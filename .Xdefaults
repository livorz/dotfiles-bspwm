
! special
*.foreground:   #e0e0e0
*.background:   #0b1f37
*.cursorColor:  #abb2bf

! black
*.color0:       #2C3E50
*.color8:       #34495E

! red
*.color1:       #C0392B
*.color9:       #E74C3C

! green
*.color2:       #01579b
*.color10:      #47D4B9

! yellow
*.color3:       #FAB795
*.color11:      #FAB795

! blue
*.color4:       #61afef
*.color12:      #61afef

! magenta
*.color5:       #8E44AD
*.color13:      #c678dd

! cyan
*.color6:       #56b6c2
*.color14:      #84e6ff

! white
*.color7:       #abb2bf
*.color15:      #ffffff


! Font
urxvt.font:             xft:Source Code Pro:Medium:pixelsize=14
!urxvt*font: xft:Terminus:size=14,xft:Inconsolata\ for\ Powerline:size=12
 !URxvt.font:                     xft:DejaVu Sans Mono:autohint=true:size=14
 !URxvt.boldFont:                 xft:DejaVu Sans Mono:autohint=true:bold:size=12
 !URxvt.italicFont:               xft:DejaVu Sans Mono:autohint=true:italic:size=12
 !URxvt.boldItalicFont:           xft:DejaVu Sans Mono:autohint=true:bold:italic:size=12


!URxvt*font:  xft:hack:size=11
!URxvt*letterSpace: 		-4

! Restore Ctrl+Shift+(c|v)
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.scrollBar: false

URxvt*depth: 32
URxvt.background: [85]#171717
